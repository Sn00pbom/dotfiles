#!/usr/bin/python3
import random

c = "b, c, d, f, g, h, j, k, l, m, n, p, q, r, s, t, v, w, x, y, z".split(', ')
cd = "bl, br, ch, cl, cr, dr, fl, fr, gl, gr, ph, pl, pr, sc, sh, sk, sl, sm, sn, sp, st, sw, th, tr, tw, wh, wr".split(', ')
cd += "nth, sch".split(', ')

v = "a, e, i, o, u, ai, au, aw, ea, ee, ei, eu, ie, oi, oo, ou".split(', ')

word = random.choice(cd) + random.choice(v) + random.choice(c)
print(word)
